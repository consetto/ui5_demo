{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.ui.demo.toolpageapp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.demo.toolpageapp.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.tnt": {},
				"sap.ui.layout": {},
				"sap.uxap": {},
				"sap.suite.ui.microchart": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.toolpageapp.i18n.i18n"
				}
			},
			"side": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/sideContent.json"
			},
			"alerts": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/alerts.json"
			},
			"customer": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/customers.json"
			},
			"process": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/data/ProcessFlowData.json"
			},
			"business": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/data/BusinessData.json"
			},
			"suppliers": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/data/Suppliers.json"
			},
			"reviews": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/data/Reviews.json"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"controlId": "mainContents",
				"viewType": "XML",
				"controlAggregation": "pages",
				"viewPath": "sap.ui.demo.toolpageapp.view",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": "home"
				},
				{
					"pattern": "MasterSettings",
					"name": "masterSettings",
					"target": [
						"detailSettings",
						"masterSettings"
					]
				},
				{
					"pattern": "SystemSettings",
					"name": "systemSettings",
					"target": "detailSettings"
				},
				{
					"pattern": "Statistics",
					"name": "statistics",
					"target": "statistics"
				},
				{
					"pattern": "Launchpad",
					"name": "Launchpad",
					"target": "Launchpad"
				},
				{
					"pattern": "OverflowToolbar",
					"name": "OverflowToolbar",
					"target": "OverflowToolbar"
				},
				{
					"pattern": "financialDashboard",
					"name": "financialDashboard",
					"target": "financialDashboard"
				},
				{
					"pattern": "processFlow",
					"name": "processFlow",
					"target": "processFlow"
				},
				{
					"pattern": "processFlow2",
					"name": "processFlow2",
					"target": "processFlow2"
				},
				{
					"pattern": "processFlow3",
					"name": "processFlow3",
					"target": "processFlow3"
				},
				{
					"pattern": "chartContainer",
					"name": "chartContainer",
					"target": "chartContainer"
				},
				{
					"pattern": "reviews",
					"name": "reviews",
					"target": "reviews"
				},
				{
					"pattern": "planningCalendar",
					"name": "planningCalendar",
					"target": "planningCalendar"
				},
				{
					"pattern": "organizationalChart",
					"name": "organizationalChart",
					"target": "organizationalChart"
				},
				{
					"pattern": "projectManagement",
					"name": "projectManagement",
					"target": "projectManagement"
				},
				{
					"pattern": "productOv1",
					"name": "productOv1",
					"target": "productOv1"
				},
				{
					"pattern": "productOv2",
					"name": "productOv2",
					"target": "productOv2"
				},
				{
					"pattern": "productOv3",
					"name": "productOv3",
					"target": "productOv3"
				},
				{
					"pattern": "deliveryRoute",
					"name": "deliveryRoute",
					"target": "deliveryRoute"
				}
			],
			"targets": {
				"home": {
					"viewId": "home",
					"viewName": "Home"
				},
				"appSettings": {
					"viewId": "appSettings",
					"viewName": "settings.AppSettings"
				},
				"masterSettings": {
					"viewId": "masterSettings",
					"viewName": "settings.MasterSettings",
					"controlId": "appSettings",
					"controlAggregation": "masterPages",
					"parent": "appSettings"
				},
				"detailSettings": {
					"viewId": "detailSettings",
					"viewName": "settings.DetailSettings",
					"controlId": "appSettings",
					"controlAggregation": "detailPages",
					"parent": "appSettings"
				},
				"statistics": {
					"viewId": "statistics",
					"viewName": "Statistics"
				},
				"Launchpad": {
					"viewName": "Launchpad"
				},
				"OverflowToolbar": {
					"viewName": "Finance.ForecastDemo.OverflowToolbar"
				},
				"financialDashboard": {
					"viewName": "Finance.Dashboard.Home"
				},
				"processFlow": {
					"viewName": "ProcessFlow"
				},
				"processFlow2": {
					"viewName": "HR.processFlow.ProcessFlow"
				},
				"processFlow3": {
					"viewName": "HR.processFlow.ProcessFlowMNodes"
				},
				"chartContainer": {
					"viewName": "ChartContainer"
				},
				"reviews": {
					"viewName": "Reviews"
				},
				"planningCalendar": {
					"viewName": "HR.PlanningCalender.Page"
				},
				"organizationalChart": {
					"viewName": "HR.OrganizationalChart.Page"
				},
				"projectManagement": {
					"viewName": "HR.ProjectManagement.BasicGanttChart"
				},
				"productOv1": {
					"viewName": "Product.ProductOv1.IconTabBar"
				},
				"productOv2": {
					"viewName": "Product.ProductOv2.View"
				},
				"productOv3": {
					"viewName": "Product.ProductOv3.View"
				},
				"deliveryRoute": {
					"viewName": "Logistic.DeliveryRoute.View"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}